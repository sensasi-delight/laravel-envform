{
    "name": "envform/laravel-envform",
    "version": "0.0.1",
    "description": "Interactive .env file generator and manager for Laravel applications using project analysis.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "env",
        "environment",
        "configuration",
        "cli",
        "interactive",
        "setup",
        "wizard",
        "analyzer",
        "generator"
    ],
    "authors": [
        {
            "name": "Zain Adam",
            "email": "zainadam.id@gmail.com"
        }
    ],
    "homepage": "https://github.com/sensasi-delight/laravel-envform",
    "support": {
        "issues": "https://github.com/sensasi-delight/laravel-envform/issues",
        "source": "https://github.com/sensasi-delight/laravel-envform"
    },
    "require": {
        "php": "^8.2",
        "laravel/prompts": "^0.3",
        "illuminate/support": "^12.0",
        "illuminate/console": "^12.0"
    },
    "autoload": {
        "psr-4": {
            "EnvForm\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "EnvForm\\EnvFormServiceProvider"
            ]
        }
    },
    "scripts": {
        "lint": [
            "phpstan analyse",
            "pint"
        ]
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require-dev": {
        "phpstan/phpstan": "^2.1",
        "laravel/pint": "^1.27"
    }
}
